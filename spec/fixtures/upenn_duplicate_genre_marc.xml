  <marc:subfield code="a">Binding:  18th-century leather over pasteboards, spine and board edges gilt, marbled endpapers.</marc:subfield>
</marc:datafield>
    <marc:datafield ind1=" " ind2=" " tag="500">
  <marc:subfield code="a">Origin:  Written in northern Italy in 1409 (f. 71r).</marc:subfield>
</marc:datafield>
    <marc:datafield ind1=" " ind2=" " tag="500">
