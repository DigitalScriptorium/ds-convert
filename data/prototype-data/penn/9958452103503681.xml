</marc:datafield>
    <marc:datafield ind1="1" ind2=" " tag="561">
  <marc:subfield code="a">Formerly owned by Harrison D. Horblit (Ridgefield, Conn.).</marc:subfield>
</marc:datafield>
    <marc:datafield ind1="1" ind2=" " tag="561">
